function setup() {
  createCanvas(1000, 1000);
  background(0,0,0);
  
 ellipse(500,500,1000,1000);
 
 fill(255,0,0);
 stroke(255,0,0);
 rect(250,250,50,50);
 rect(750,750,50,50);
 rect(750,250,50,50);
 rect(250,750,50,50);
 
 fill(0,255,0);
 stroke(0,255,0);
 ellipse(500,500,400,400);
 
 fill(0,0,255);
 stroke(0,0,255);
 ellipse(500,500,250,250);
 
 line(300,300,750,750);

 
}


