var x;
var y;

function setup() {
  createCanvas(windowWidth,windowHeight);
  x = windowWidth/2;
  y = windowHeight/2;
  background(random(0,255));
}

function draw() {
  SuperSpecialAwesome(x,y);

}
function SuperSpecialAwesome(x,y) {
    fill(random(0,255));
    rect(x,y,300,300);
    fill(random(0,255));
    ellipse(x,y,100,100);
}   
